\FB<README.md>\fp
\FB<AUTHORS>\fp
\FB<funtions.c>\fp
\FB<funtions2.c>\fp
.PP
\int _strncmp(char *s1, char *s2, size_t n);
.PP
char *_strcat(char *dest, char *src);
.PP
int _strlen(char *s);
.PP
char *_strdup(char *str);
.PP
void execute(char *av[], char **env);
.PP
char *_path(char av[], char *env[]);
.PP
int _strcmp(char *s1, char *s2);\fP
.PP
